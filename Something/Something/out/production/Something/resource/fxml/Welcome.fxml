    <stylesheets>
        <URL value="@../css/Welcome.css" />
    </stylesheets>
    <Button layoutX="180.0" layoutY="288.0" minHeight="100" mnemonicParsing="false" onAction="#startgame" text="Start" />
    <Button layoutX="175.0" layoutY="206.0" minHeight="50" mnemonicParsing="false" onAction="#startScore" text="Score" />
    <Button fx:id="exitButton" layoutY="52.0" mnemonicParsing="false" text="Button" />
</AnchorPane>
